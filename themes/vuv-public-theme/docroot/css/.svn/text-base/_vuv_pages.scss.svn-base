                    margin: 40px 0 0 0;
                }
                 @include tablet {
                    width:100%;
                    float:none;
                    margin: 30px 0 0 0;
                    text-align: center;
                }

            }//span8 
        }//.form-inline

        #search {
            @include phone {
                padding-bottom: 20px !important;
            }
        }
    }//find-services
    /*Jquery slider*/
    #servTemp-userCompanyOptions{
        display: none;
        @include desktop {
            &.row-fluid [class*="span"] {
                margin-left: 0;
            }
        }
        @include phone {

        }
    }

    #search {
      
         .span1 .fa {
            // margin-left:5px;
        
            @include phone {
