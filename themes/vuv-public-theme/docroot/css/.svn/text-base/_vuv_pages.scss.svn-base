                &:after {
                     @include fadeInBluel-bt;

                    }//after
            }//hover
        }
        
        @include phone {
            width:auto;
            margin: 30px 15px 0;
            margin-bottom:0 !important;
            padding-bottom:74px;
            min-height: auto;
            
        }
        &:hover {
            background: $vuvBlue-hl;
        }
        h4{
            text-align: left;
        }

        .serviceDescrip {
            margin-top: 20px;
        }
        .btn {
            position:absolute;
            bottom:15px;
            right:15px;
            z-index: 20;
            background: rgba($vuvGrey-ml, 0.3);
            &:hover {
                background: $vuvBlue-l;
            }
            span {
                margin-left: 5px;
                font-weight: 200;
                font-size: 10px;
            }

        }
    }//#ficha

