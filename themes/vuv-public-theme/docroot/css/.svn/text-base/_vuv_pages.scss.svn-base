    }//#search
    
    .age-slider.pull-right.span8 {
        @include clear-all;
        height:50px;
        #amount {
            display: none;
        }
        @include phone {
            width:100%;
            float:none;
            margin-top: 50px;
            margin-bottom:40px;
            @include clear-all;
            
            *{
                box-sizing:border-box;
            }
            .span1 {
                width:5%;
                display: inline-block;
                vertical-align: top;
                float: left;

            }
            .span10 {
                display: inline-block;
                width:90%;
                float: left;
                padding: 0 4px;
            }
        }//include tablet
    }//.age-slider

    #radiogroupUser {
            margin-left: 0;
            @include tablet{
                width: auto;
                float: none;
                display: inline-block;
                margin-left:5px;
            }
            @include phone{
                width: auto;
                float: none;
                display: inline-block;
                margin-left:30px;
            }
        }
    #sex-selector {
        margin-left: 0;
        margin-right: 15px;
        @include tablet{
            width:auto;
            float:none;
            display:inline-block;
            margin: 0 ;
        }
        @include phone{
            width:auto;
            float:none;
            display:inline-block;
            margin: 0 ;
        }
        .btn {
            border: 1px solid darken($vuvGrey-ui, 10%);
            &:hover{
                border-color: $vuvBlue-l;
                color:#fff;
            }
            &.active {
                background: $vuvBlue-l;
                color:#fff;
            }
            &:nth-child(2) {
                &:hover{
                    background: #e565cb !important;
                    border-color: #e565cb !important;
                }
                &.active {
                    background: #e565cb !important;
                    border-color: #e565cb !important;
                }
            }
        }//btn

    }//sex-selector
    #age-selector {

        .fa {
            margin-top: 5px;
        }
        .ui-slider-handle {
            font-size: 12px;
        }
        &.ui-widget-content {
            background: $vuvGrey-ui;
            border: 1px solid darken($vuvGrey-ui, 10%);
        }//bar widget content

        &.ui-slider-horizontal {
            height:4px;
            margin-top: 5px;
        }
        
        &.ui-slider-horizontal .ui-slider-handle {
            width:14px;
            height:14px;
            box-shadow: 0 1px 3px 0 rgba($vuvGrey-m, .3);
            margin-left: -7px;
            margin-top: -2px;
            cursor:pointer;
            @include phone{
                width:22px;
                height: 22px;
                margin-left: -10px;
                margin-top: -7px;
            }
            &.ui-state-focus {
                
            }
        }
        &.ui-slider-pips .ui-slider-pip {
           
            top:15px;
            @include phone {
                top:18px;
            }
            
            .ui-slider-label {
                top:5px;
                margin-left: -10px;
            }
        }

        .ui-slider-label {
             font-size: 11px;
            color:#999;
            font-weight:400;

        }
        .ui-slider-pip-selected-1, .ui-slider-pip-selected-2 {
              .ui-slider-label {
                color:$vuvBlue-l;
                font-weight: 600;
              }
        }
        > span:last-child .ui-slider-label {
            margin-left:-7px;
            &:after {
                content:"+";
            }//after
        }//:last-child .ui-slider-label

    }//age-selector
   
    .companyOptions {
        font-size: initial !important;
        font-weight: initial !important;
    }
    //#ficha
   .span4.well.well-small.ficha-item {
