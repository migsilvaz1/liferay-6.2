/* 0.1.0 Home
========================================================================================== */

/* -------------------
0.1.1 Services Single 
------------------- */

#services-list {
    text-align: center;
    margin-top: 20px;
    &:after {
        content: "";
        display: "table";
        clear: both;
    }
    .services {
        text-align: left;
        .home & {
            margin-top: 30px;
        }
        &:before {
            background-position: left bottom;
            background-repeat: no-repeat;
            content: "";
            display: block;
            height: 70px;
            width: 100%;
        }
        &.service-one:before {
             @include vuv-background-image('services_icons/vuv_ico_srv_transport');
         }
        &.service-two:before {
             @include vuv-background-image('services_icons/vuv_ico_srv_disabled');
           
        }
        &.service-three:before {
             @include vuv-background-image('services_icons/vuv_ico_srv_education');
           
        }
        &.service-four:before {
             @include vuv-background-image('services_icons/vuv_ico_srv_financial');
            
        }
        &.service-five:before {
             @include vuv-background-image('services_icons/vuv_ico_srv_legal');
           
        }
        &.service-six:before {
             @include vuv-background-image('services_icons/vuv_ico_srv_property');
           
        }
    }
    h3 {
        font-size: 19px;
        line-height: 23px;
        
        a {
            color: $vuvBlue-l;
            font-weight: 600;
            &:hover {
                text-decoration: underline;
                display: block;
            } //hover
        } //a
    } //h3
    .btn {
        margin: 50px auto 80px;
        
        @include phone {
            display: inline-block;
            width: auto;
        }
    }
} //#services-list


/* -------------------
0.1.2 News Grid 
------------------- */

.news-list {
    position: relative;
    &:after {
        content: "";
        display: table;
        clear: both;
    }

    .home & {
    	padding-top: 80px;
    	&:before {
	        content: "";
	        position: absolute;
	        width: 6000px;
	        top: 0;
	        margin-left: -50%;
	        height: 100%;
	        background: #f2f2f2;
	        z-index: -1;
            @include tablet {
                display: none;
            }
            @include phone {
                display: none;
            }

    	} //before
	}//home .news-list

    .span4 .news-item:last-child {
        border-bottom: 0 none;
    } //last-child

    .news-item {
        padding-bottom: 30px;
        border-bottom: 1px solid #d4d4d4;
        margin-top: 30px;
        @include phone{
            border-bottom: 0 none;
        }
        .meta {
            line-height: 20px;
            padding: 10px 0 5px;
            color: $vuvGrey-m;
        } //meta
        
        h2 {
            font-size: 20px;
            line-height: 23px;
            margin: 0 0 5px;
            a {
                color: $vuvGrey-d;
                font-weight: 600;
                &:hover {
                    color: $vuvBlue-l;
                } //hover	
            } //a
        } //h2
    } //news-item

    .highlights {
        padding-bottom: 50px;
        border:0 none;
        margin-bottom: 0;

        .span4 > img {
            @include tablet {
                margin-top: 0px;
            }
            @include phone {
                margin-top: 20px;
            }
        }
        .news-item {
                margin-top: 0px;
            }
        
        .home & {
            border-bottom: 1px solid #d4d4d4;
            margin-bottom: 20px;
            @include phone {
                border-bottom: 0 none;
            }

        }

        &:after {
            content: "";
            display: table;
            clear: both;
        }//:after

        .span6 > .main-news {
            margin-bottom: 30px;
        }

        .main-news {
            position: relative;
            .news-item{
                margin-top: 0;
            }
            > img {
                width: 100%;
            }
            img {
                width:100%;
            }
            .content {
                position: absolute;
                top: 0;
                left: 0;
                height: 100%;
                width: 50%;
                color: #fff;
                @include fadeInBlack;

                @include phone {
                    position:relative;
                    background: none;
                    width:100%;

                    .txt-box {
                        color:$vuvGrey-m;
                    }//txt-box
                } //iphone

                @include tablet {
                    position:relative;
                    background: none;
                    width:100%;
                   
                    .txt-box {
                        color:$vuvGrey-m;
                    }
                }             
                
                .meta {
                    line-height: 20px;
                    padding: 5px 20px;
                    color: #ebebeb;
                    background: #000;
                    display: inline-block;
                    @include phone {
                        display: block;
                    }
                    @include tablet {
                       display: block;
                    }
                } //meta

                h2 {
                    background: rgba($vuvBlue-org, .7);
                    padding: 20px;
                    margin: 0;
                    display: block;
                    font-size: 28px;
                    line-height: 34px;
                    font-weight: 500;
                    a {
                        color: #fff;
                    }
                    @include tablet {
                        font-size: 20px;
                        line-height: 24px;
                    }
                    @include phone {
                        font-size: 20px;
                        line-height: 24px;
                    }
                } //h2
                .txt-box {
                    padding: 10px 20px;
                    font-size: 16px;
                    a {
                        color: #97d1ff;
                    } //a
                } //.txt-box
                &.w60 {
                    width: 60%;
                } //w60;	
            } //content
        } //main-news
    } //highlights

    @include phone {
        .news-item {
            .header, .txt-box{
                padding-left: 15px;
                padding-right: 15px;
            }
        }
    }
} //newslist

/* -------------------
0.2.0 News Single 
------------------- */

.meta.utilities {
    margin-top: 50px;
    margin-top: 10px;
    text-align: left;
    color: $vuvGrey-ld;
    @include phone{
        padding-left: 15px;
        padding-right: 15px;
    }
    &:after {
        content: "";
        display: table;
        clear: both;
    }
    .cat-label {
        color: $vuvGrey-m !important;
    }
    div,
    span {
        display: inline-block;
        margin-left: 20px;
        line-height: 20px;
        padding: 10px 0;
        &.share {
            margin-left: 0;
        }
        &.comments {
            &:hover {
                color: $vuvGrey-l;
                // border-bottom:1px solid $vuvGrey-l;	
            }
        }
        &.article-info {
            float: right;
        }
        &.date {
            clear: both;
            display: block;
            border-top: 1px solid #d4d4d4;
            margin-left: 0;
        }
        .fa {
            font-size: 16px;
            cursor: pointer;
            &:hover {
                color: $vuvGrey-l;
                // border-bottom:1px solid $vuvGrey-l;
            } //hover
        } //fa
    } //span
} //utilities

.news-single {
    position: relative;
    padding-top: 0;
    @include phone{
        padding-left: 15px;
        padding-right: 15px;
    }
    // margin-right: -15px;
    // padding-right: 30px;
    // border-right: 1px solid #eee;

    .art-image {
        margin-top: 30px;
        background: #f6f6f6;
        position: relative;
        @include phone{
            margin-top:20px;
        }
        &:after {
            content: "";
            display: table;
            clear: both;
        } //after
        img {
            display: inline-block;
            width: 70%;
            max-width: none;
            @include phone {
                width:100%;
            }
        }
        .caption {
            float: right;
            box-sizing: border-box;
            padding: 18px;
            font-size: 12px;
            line-height: 16px;
            width: 190px;
            width: 30%;
            @include phone{
                width: 100%;
                float:none;
            }
        }
        .copyright {
            display: block;
            margin-top: 10px;
            text-transform: uppercase;
        }
    } //art-image
    .more-images {
        display: block;
        margin-top: 10px;
        color: $vuvGrey-l;
        i {
            margin-left: 5px;
        }
    } //more-images
    .more-photos {
        font-weight: 700;
    }
    .art-content {
        margin-top: 20px;
        p {
            // margin-top: 30px;
        }
        h3 {
            font-size: 19px;
            line-height: 24px;
            // margin: 
        }
    } //art-content
} //news-single

/* -------------------
0.2.1 News List View
------------------- */

.news-item.listview {
    border-bottom: 1px solid $vuvGrey-l;
    padding: 20px;
    &:first-child {
        border-top: 1px solid $vuvGrey-l;
    }
    &:after {
        content: "";
        display: "table";
        clear: both;
    }
    img {
        width: 160px;
        height: auto;
        float: left;

        display: block;
    }
    .header,
    .txt-box {
        margin-left: 180px;
    }
    h2 {
        font-size: 19px;
        line-height: 23px;
        margin: 0 0 4px;
    }
    .date {
        color: $vuvGrey-m;
    }
    .cat-label {
        font-size: 11px;
    }
} //news-item.listview




/* 0.3.0 Institutions
========================================================================================== */

.aui {
    .mobile-select-wrapper {
        display: none;
        @include phone {
            // display: block;
            display:none;
            select {
                width: 290px;
            }
        }//phone
    }//.mobile-select-wrapper
    .institutions-wrapper {
        position: relative;
        padding-top: 20px;
        padding-bottom:50px;
        padding-top: 0;
        @include clear-all;
        
        #search {
            margin-bottom: 0;
        }//#search




        .page-header {
            padding-bottom:20px;
            margin-bottom: 0;
            p{
                font-size: 17px;
                line-height: 25px;
            }

        }
        h4{
            font-size:19px; 
            line-height: 23px;
            margin-top: 30px;
        }
        h5{
            font-size:14px;
            font-weight: 700;
            color:$vuvGrey-m;
        }
        .online {
            color: $vuvGreen-ui;
            &:before {
                @include fa-alike;
                content:"\f00c";
                margin-right: 4px;
                font-weight: 400;
            }

        }


    }//intitution wrapper

    #list.institutions-list {
        position: relative;
        height: 100%;
        left: 0;
        top: 0;
        bottom: 0;
        border-right: 1px solid $vuvGrey-ui;
        box-sizing: border-box;
        
        @include phone {
            height: 141px; //27 * 3
            overflow-y: auto;
            border-bottom: 1px solid $vuvGrey-ui;
            box-shadow: 0 0 10px 0 rgba($vuvGrey-ui, .5) inset;
        }//phone
    }
    #institutions {
        position: relative;
        &:before {
            content: "";
            display: block;
            width: 1px;
            height: calc(100% + 50px);
            height: 100%;
            position: absolute;
            background: $vuvGrey-ui;
            left: -21px;
            // top: -50px;
            top:0;
            bottom: 0;
            // @media only screen and (max-width: 1200px) {
            //}@media
            @include desktop {
                left: -31px;
            }//desktop     
        }//before
    }//institutions

    #search,
    #list {
        border-color: $vuvGrey-ui;
    }
    

    
    .institutions-list {
        list-style-type: none;
        margin: 0;
        padding: 0;
        li {
            padding: 10px 20px 10px 8px;
            margin: 0;
            position: relative;
            border-bottom: 1px solid $vuvGrey-ui;
            cursor: pointer;
            display: block;
            &:last-child{
                border-bottom: 0 none;
            }
            &:before {
                @include fa-alike;
                content: "\f105";
                margin-right: 8px;
            } //before
            &:hover {
                background: $vuvBlue-hl;
            }
            &.active {
                background: $vuvGrey-l;
                color: $vuvGrey-d;
                font-weight: 600;
                &:before {
                    // content:" ";
                    color: $vuvGrey-l;
                }
                &:after {
                    @include fa-alike;
                    content: "\f105";
                    // position:absolute;
                    // right:10px;
                    float: right;
                    font-weight: 600;
                    line-height: 20px;
                    font-size: 14px;
                    @include phone {
                        content: "\f107"; 
                    }
                }//after
            }//active
        } //li
    }//institution-list
    
    .inst-content-wrapper {
        max-width: 730px;
        padding-left: 10%;
        padding-top: 30px;
        .alert{
            display: inline-block;
            padding: 8px 14px;
        } //alert
        @include phone {
            padding: 30px 15px;
        }      
    }//inst-content-wrapper

    /* 0.4.0 Servicios
    ========================================================================================== */
    #servicesList {
        padding-bottom: 30px;
        margin-left:0 !important;
        .servicesRowList {
            margin-left: 0 !important;
        }
    }
    .find-services {
        .fa{
            min-width:18px;
        }
        
        .form-inline {

            @include phone {
